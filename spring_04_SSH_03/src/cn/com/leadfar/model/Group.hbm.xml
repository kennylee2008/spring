<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.com.leadfar.model.Group" table="t_group">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="name"/>
		<set name="persons" lazy="true" inverse="true">
			<key column="gid"></key>
			<one-to-many class="cn.com.leadfar.model.ContactPerson"/>
		</set>
	</class>
</hibernate-mapping>